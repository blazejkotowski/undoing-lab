---
layout: base
---

<h1>{{ page.title }}</h1>

<p>
  {{ page.content | markdownify }}
</p>

{% for image in page.images %}
  <img src={{ image.src | prepend: '/assets/images/' }} alt="">
{% endfor %}

{% if page.previous %}
  <a href={{ page.previous.url }}>
    <h6>{{ page.previous.title }}</h6>
  </a>
{% endif %}

{% if page.next %}
  <a href={{ page.next.url }}>
    <h6>{{ page.next.title }}</h6>
  </a>
{% endif %}